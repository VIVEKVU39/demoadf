							"enableLogging": true,
							"storeSettings": {
								"type": "AzureBlobStorageReadSettings",
								"fileListPath": "adfdemo/output/testadf.csv",
								"enablePartitionDiscovery": false
							}
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"annotations": [],
				"lastPublishTime": "2023-03-20T20:39:20Z"
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/outputfile')]",
				"[concat(variables('factoryId'), '/linkedServices/AzureBlobStorage1linkedservice')]"
			]
		},
