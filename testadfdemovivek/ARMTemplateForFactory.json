		"trigger2_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/2af608f7-b160-4113-85c8-f1b6bee02649/resourceGroups/Testenv/providers/Microsoft.Storage/storageAccounts/storagevivekadf"
